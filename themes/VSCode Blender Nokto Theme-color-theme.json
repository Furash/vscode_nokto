{
	"name": "VSCode Blender Nokto Theme",
	"type": "dark",
	"colors": {

		"focusBorder": "#1A2226",
		"selection.background": "#212B30",
		"widget.shadow": "#000000",
		
		// INPUTS
		"input.background": "#2F3D44",
		"input.foreground": "#C3CBCD",
		"input.placeholderForeground": "#72acc7",
		"inputOption.activeBorder": "#008D80",
		"dropdown.background": "#212B30",
		"button.background": "#009688",

		//EDITOR
		"editor.background": "#222d32",
		"editor.foreground": "#ffffffdd",

		// WINDOW
		"window.activeBorder": "#273237",
		"window.inactiveBorder": "#273237",

		// STATUS BAR
		"statusBar.foreground": "#8BB8CC",
		"statusBar.debuggingBackground": "#75715e",
		"statusBar.background": "#222D32",
		"statusBar.noFolderBackground": "#212B30",

		// ACTIVITY BAR
		"activityBarBadge.background": "#273237",
		"sideBarTitle.foreground": "#bbbbbb",
		"activityBar.background": "#222D32",
		"activityBar.foreground": "#8BB8CC", // + icons
		"debugToolBar.background": "#1e1f1c",
		"sideBar.foreground": "#72acc7",
		"sideBar.background": "#222D32",
		"sideBarSectionHeader.background": "#222D32",
		"sideBarSectionHeader.foreground": "#8BB8CC",
		"titleBar.activeBackground": "#222D32",

		// PEEK VIEW
		"peekView.border": "#212B30",
		"peekViewTitle.background": "#222D32",
		"peekViewEditor.background": "#071A26",
		"peekViewResult.background": "#071A26",
		"tab.inactiveBackground": "#222D32",
		"tab.border": "#263238",

		// SEARCH RESULTS
		"editor.findMatchBackground": "#008D8050",
		"editor.findMatchHighlightBackground": "#008D8050",
		"editor.findRangeHighlightBackground": "#008D8050",

		// TAB
		"tab.activeBackground": "#435763",
		"tab.activeBorder": "#222D32",
		"tab.inactiveForeground": "#72acc7",
		"tab.unfocusedInactiveForeground": "#72acc7",

		"editorGroupHeader.tabsBackground": "#222D32",
		"editorGroup.border": "#212B30",
		"editorGroup.dropBackground": "#212B3080",
		"panel.border": "#212B30",
		"panelTitle.activeForeground": "#38381a",
		"panelTitle.activeBorder": "#00BCD4",

		// ERRORS
		"editorError.foreground": "#CD3131",
		"list.errorForeground": "#CD3131",
		"inputValidation.errorBackground": "#680000",
		"inputValidation.errorBorder": "#CD3131",

		// WARNINGS
		"list.warningForeground": "#FFCC00",
		"editorWarning.foreground": "#FFCC00",
		"inputValidation.warningBackground": "#7c6300",
		"inputValidation.warningBorder": "#FFCC00",

		// INFOS
		"inputValidation.infoBackground": "#006259",
		"inputValidation.infoBorder": "#008D80",

		// GIT
		"diffEditor.insertedTextBackground": "#66852880",
		"diffEditor.removedTextBackground": "#BC001380",
		"gitDecoration.untrackedResourceForeground": "#009688",
		"gitDecoration.modifiedResourceForeground": "#8BB8CC",
		"gitDecoration.deletedResourceForeground": "#FFA3A3",
		"gitDecoration.conflictingResourceForeground":"#ff001e80",
		"gitDecoration.ignoredResourceForeground": "#72acc7",
		
		// TREE AND LISTS
		"list.focusBackground": "#3A464C",
		"list.focusForeground": "#FFFFFF",
		"list.activeSelectionBackground": "#00BCD4",
		"list.activeSelectionForeground": "#FFFFFF",
		"list.inactiveSelectionBackground": "#1B2326",
		"list.hoverBackground": "#3A464C", // "#00BCD4",
		"list.hoverForeground": "#FFFFFF",
		"list.dropBackground": "#1B2326",
		"list.highlightForeground": "#f8f8f2",
		"scrollbar.shadow": "#00000000",
		"pickerGroup.foreground": "#009688",
		"progressBar.background": "#00BCD4",
		"listFilterWidget.background": "#222D32",
		"listFilterWidget.outline": "#A6E22E",
		"listFilterWidget.noMatchesOutline": "#ff0000",

	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment",
				"string.quoted.docstring.multi.python",
				"punctuation.definition.string.end.python",
				"punctuation.definition.string.begin.python",
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#546E7A"
			}
			
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#ffffffdd"
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#ffffffdd"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#ff0000"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#00ffffaa"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"keyword.control",
				"constant.other.color",
				"punctuation",
				"meta.tag",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.section.embedded",
				"keyword.other.template",
				"keyword.other.substitution"
			],
			"settings": {
				"foreground": "#00fefebb"
			}
		},
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#43fda2aa"
			}
		},
		{
			"name": "Function, Special Method",
			"scope": [
				"variable.function",
				"support.function",
				"keyword.other.special-method"
			],
			"settings": {
				"foreground": "#00fefebb"
			}
		},
		{
			"name": "Function",
			"scope": [
				"entity.name.function",
			],
			"settings": {
				"foreground": "#ffffffdd"
			}
		},
		{
			"name": "Function Call",
			"scope": [
				"meta.function-call",
			],
			"settings": {
				"foreground": "#ffadfbc2"
			}
		},
		{
			"name": "Block Level Variables",
			"scope": [
				"meta.block variable.other"
			],
			"settings": {
				"foreground": "#f07171"
			}
		},
		{
			"name": "Other Variable, String Link",
			"scope": [
				"support.other.variable",
				"string.other.link"
			],
			"settings": {
				"foreground": "#e571f0aa"
			}
		},
		{
			"name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
			"scope": [
				//"constant.numeric",
				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape",
				"variable.parameter",
				"keyword.other.unit",
				"keyword.other"
			],
			"settings": {
				"foreground": "#ffffffdd"
			}
		},
		{
			"name": "String, Symbols, Inherited Class, Markup Heading",
			"scope": [
				"constant.numeric", //from above
				"string",
				"constant.other.symbol",
				"constant.other.key",
				"entity.other.inherited-class",
				"markup.heading",
				"markup.inserted.git_gutter",
				"meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				"foreground": "#43ffa4aa"
			}
		},
		{
			"name": "Class, Support",
			"scope": [
				"entity.name",
				"support.type",
				"support.class",
				"support.other.namespace.use.php",
				"meta.use.php",
				"support.other.namespace.php",
				"markup.changed.git_gutter",
				"support.type.sys-types"
			],
			"settings": {
				"foreground": "#ff8af3"
			}
		},
		{
			"name": "Entity Types",
			"scope": [
				"support.type"
			],
			"settings": {
				"foreground": "#B2CCD6"
			}
		},
		{
			"name": "CSS Class and Support",
			"scope": [
				"source.css support.type.property-name",
				"source.sass support.type.property-name",
				"source.scss support.type.property-name",
				"source.less support.type.property-name",
				"source.stylus support.type.property-name",
				"source.postcss support.type.property-name"
			],
			"settings": {
				"foreground": "#B2CCD6"
			}
		},
		{
			"name": "Sub-methods",
			"scope": [
				"entity.name.module.js",
				"variable.import.parameter.js",
				"variable.other.class.js"
			],
			"settings": {
				"foreground": "#ff8af3"
			}
		},
		{
			"name": "Language methods",
			"scope": [
				"variable.language"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#ff8af3"
			}
		},
		{
			"name": "entity.name.method.js",
			"scope": [
				"entity.name.method.js"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#82AAFF"
			}
		},
		{
			"name": "meta.method.js",
			"scope": [
				"meta.class-method.js entity.name.function.js",
				"variable.function.constructor"
			],
			"settings": {
				"foreground": "#82AAFF"
			}
		},
		{
			"name": "Attributes",
			"scope": [
				"entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#C792EA"
			}
		},
		{
			"name": "HTML Attributes",
			"scope": [
				"text.html.basic entity.other.attribute-name.html",
				"text.html.basic entity.other.attribute-name"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#FFCB6B"
			}
		},
		{
			"name": "CSS Classes",
			"scope": [
				"entity.other.attribute-name.class"
			],
			"settings": {
				"foreground": "#FFCB6B"
			}
		},
		{
			"name": "CSS ID's",
			"scope": [
				"source.sass keyword.control"
			],
			"settings": {
				"foreground": "#82AAFF"
			}
		},
		{
			"name": "Inserted",
			"scope": [
				"markup.inserted"
			],
			"settings": {
				"foreground": "#C3E88D"
			}
		},
		{
			"name": "Deleted",
			"scope": [
				"markup.deleted"
			],
			"settings": {
				"foreground": "#ff00e5"
			}
		},
		{
			"name": "Changed",
			"scope": [
				"markup.changed"
			],
			"settings": {
				"foreground": "#C792EA"
			}
		},
		{
			"name": "Regular Expressions",
			"scope": [
				"string.regexp"
			],
			"settings": {
				"foreground": "#89DDFF"
			}
		},
		{
			"name": "Escape Characters",
			"scope": [
				"constant.character.escape"
			],
			"settings": {
				"foreground": "#89DDFF"
			}
		},
		{
			"name": "URL",
			"scope": [
				"*url*",
				"*link*",
				"*uri*"
			],
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"name": "Decorators",
			"scope": [
				"tag.decorator.js entity.name.tag.js",
				"support.type.python",
				"entity.name.function.decorator.python",
				"punctuation.definition.decorator.python",
				"tag.decorator.js punctuation.definition.tag.js"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#f78cf4da"
			}
		},
		{
			"name": "ES7 Bind Operator",
			"scope": [
				"source.js constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#ff00e5"
			}
		},
