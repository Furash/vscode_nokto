	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment",
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#546E7A",
			}
			
		},
		{
			"name": "Comment",
			"scope": [
				// "comment",
				"string.quoted.docstring.multi.python",
				"punctuation.definition.string.end.python",
				"punctuation.definition.string.begin.python",
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#51b97d"
			}
			
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#ffffffdd"
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#ffffffdd"
			}
		},
